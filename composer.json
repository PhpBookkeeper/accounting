{
    "name": "php-bookkeeper/accounting",
    "type": "library",
    "description": "Accounting system",
    "keywords": [
        "accounting"
    ],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/php-bookkeeper/accounting/issues?state=open",
        "source": "https://github.com/php-bookkeeper/accounting"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "moneyphp/money": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "ramsey/uuid": "^4.1",
        "roave/infection-static-analysis-plugin": "^1.6",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Bookkeeper\\Accounting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vjik\\Accounting\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    }
}
